.page{
	width: 75%;
	margin: 60px auto 0;
	min-height: 999px;
}
.main-content{
	width: 85%;
	margin: 0 auto;
}
.content-list li{
	height: 60px;
	line-height: 81px;
	border-bottom: 1px solid #ccc;
	color: #494949;
}
.content-list li:hover{
	color: #c6b74f;
}
.pro-cap{
	display: inline-block;
	width: 58%;
	font-size: 18px;
}
.pro-cot{
	display: inline-block;
	width: 41%;
	color: #7e7e7e;
	position: relative;
}
.pro-time{
	position: absolute;
	right: 0;
	top: 0;
}